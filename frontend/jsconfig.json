{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "shared/*": ["src/shared/*"],
      "shared/components/*": ["src/shared/components/*"],
      "shared/constants/*": ["src/shared/constants/*"],
      "shared/contexts/*": ["src/shared/contexts/*"],
      "shared/hooks/*": ["src/shared/hooks/*"],
      "shared/data/*": ["src/shared/data/*"],
      "store/*": ["src/store/*"],
      "selector/*": ["src/selector/*"],
      "assets/*": ["src/assets/*"],
      "@/*": ["./*"]

    }
  },
  "include": ["src"]
}
